{
  "name": "layout-manager",
  "version": "0.1.4",
  "description": "Generic library to manage the layout.",
  "maintainers": [
    {
      "name": "Thomas CHEVALIER",
      "email": "thomas.chevalier@atome.io"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jasmine-reporters": "^2.2.1",
    "jest": "^19.0.2",
    "jsdoc": "^3.5.5",
    "minimist": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "scripts": {
    "clean": "rm -rf dist lib",
    "watch": "webpack --progress --colors --watch",
    "test": "jest",
    "build": "webpack",
    "build-examples": "webpack --env=examples",
    "watch-examples": "webpack --progress --colors --watch --env=examples",
    "lib": "babel src --out-dir lib -s",
    "docs": "jsdoc -c jsdoc.json -r",
    "preversion": "npm run clean && npm test",
    "version": "npm run build && npm run lib",
    "postversion": "git push && git push --tags"
  },
  "author": {
    "name": "Thomas CHEVALIER",
    "email": "thomas.chevalier@atome.io"
  },
  "main": "./lib/index.js",
  "style": "./css/layout.css",
  "files": [
    "dist/",
    "lib/",
    "css/",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/atome-fr/layout-manager.git"
  },
  "keywords": [
    "manager",
    "layout"
  ],
  "jest": {
    "roots": [
      "__tests__"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,jsx}"
    ],
    "coverageDirectory": "out/coverage/",
    "coverageReporters": [
      "lcov",
      "cobertura",
      "text"
    ],
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jasmine-env.js"
  }
}
